{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "esnext",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "paths": {
      "pages": [
          "./src/pages"
      ],
      "@/*": ["./src/*"]
    }
  },
}